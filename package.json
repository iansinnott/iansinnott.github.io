{
  "name": "blog.iansinnott.com",
  "private": true,
  "description": "The personal blog of Ian Sinnott",
  "version": "4.0.0",
  "engines": {
    "node": ">=8.3.0"
  },
  "author": "Ian Sinnott <ian@iansinnott.com> (https://www.iansinnott.com)",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "core-js": "^2.5.0",
    "date-fns": "^1.28.5",
    "font-awesome": "^4.7.0",
    "gatsby": "1.6.5",
    "gatsby-link": "1.6.5",
    "gatsby-plugin-google-analytics": "^1.0.3",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "gatsby-plugin-stylus": "^1.1.1",
    "gatsby-remark-autolink-headers": "^1.4.4",
    "gatsby-remark-prismjs": "^1.2.1",
    "gatsby-source-filesystem": "^1.4.2",
    "gatsby-transformer-remark": "^1.7.0",
    "graphql": "0.10.5",
    "mdify": "^1.0.7",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react-disqus-comments": "^1.0.3",
    "rupture": "^0.6.2",
    "strip-indent": "^2.0.0",
    "unist-util-visit": "^1.1.3"
  },
  "keywords": ["gatsby", "blog"],
  "main": "n/a",
  "scripts": {
    "draft": "./scripts/create-doc.js draft",
    "post": "./scripts/create-doc.js post",
    "standardizeFrontmatter": "for post in $(find $PWD/content/_posts -iname '*.md'); do ./scripts/validate-frontmatter.js $post; done",
    "build": "NODE_ENV=production gatsby build",
    "postbuild": "echo 'blog.iansinnott.com' > public/CNAME",
    "start": "gatsby develop",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d public --branch master",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "prettier": "^1.5.2"
  }
}
