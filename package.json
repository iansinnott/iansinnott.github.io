{
  "name": "blog.iansinnott.com",
  "private": true,
  "description": "The personal blog of Ian Sinnott",
  "version": "4.0.0",
  "engines": {
    "node": ">=8.3.0"
  },
  "author": "Ian Sinnott <ian@iansinnott.com> (https://www.iansinnott.com)",
  "dependencies": {
    "@iansinnott/gatsby-source-notion-collection": "^0.1.11",
    "classnames": "2.2.5",
    "date-fns": "1.28.5",
    "font-awesome": "^4.7.0",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.3",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-google-analytics": "^2.1.36",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-plugin-stylus": "*",
    "gatsby-remark-autolink-headers": "^1.4.4",
    "gatsby-remark-prismjs": "^1.2.1",
    "gatsby-source-filesystem": "^2.1.0",
    "gatsby-source-graphql": "^2.1.0",
    "gatsby-transformer-remark": "*",
    "graphql": "^14.4.0",
    "graphql-tag": "^2.10.1",
    "mdify": "^1.0.7",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^16.8.6",
    "react-disqus-comments": "^1.0.3",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "rupture": "^0.6.2",
    "strip-indent": "^2.0.0",
    "unist-util-visit": "^1.1.3"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "main": "n/a",
  "scripts": {
    "draft": "./scripts/create-doc.js draft",
    "post": "./scripts/create-doc.js post",
    "standardizeFrontmatter": "for post in $(find $PWD/content/_posts -iname '*.md'); do ./scripts/validate-frontmatter.js $post; done",
    "install-build": "yarn && yarn run build",
    "build": "NODE_ENV=production gatsby build",
    "postbuild": "echo 'blog.iansinnott.com' > public/CNAME",
    "start": "gatsby develop",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d public --branch master",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "prettier": "^2.0.0"
  }
}
